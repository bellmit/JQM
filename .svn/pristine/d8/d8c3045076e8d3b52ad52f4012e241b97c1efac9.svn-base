@charset "utf-8";
/*CSS Document*/
/*reset*/
*{margin:0;padding:0;}
body{font-size:12px;font-family:Verdana, Simsun;}
ul,li{list-style:none;}
.mini-fit{min-height:150px !important;}

/*default*/
.content{
   /* min-height:450px;*/
    width:100%;
    border: 1px solid #C2C2C2;
    padding-bottom:10px;
}
.header{
    width:100%;
    height:30px;
    line-height:30px;
    border-bottom: 1px solid #C2C2C2;
    background: #E5E5E5;
}
.main{
	position:relative;
    box-sizing: border-box;
    width:100%;
    height:100%;
    /*border: 1px solid #C2C2C2;*/
    background: #F4F4F4;
    left:0 !important;
}
.info_name{
    display: inline-block;
    color:#777777;
    width:140px;
}
.info_name_type{
    display: inline-block;
    color:#777777;
}
em{
    color:red;
    margin-left:5px;
}

.form_list>li{
    float:left;
    margin:0 35px 14px 0;
}
.form_file{
	height:185px;
}
.form_file>li{
	float:left;
    width:144px;
}

/*隐藏分页栏
.mini-grid-pager{
	display:none !important;
}
*/
/*表头高度*/
.mini-grid-table{
	height:44px;
}
/*表头样式*/
.mini-grid-headerCell{
	background:#DEE6F1 !important;
	border-bottom-color:#99BCE8;
}
/*表格外边框颜色*/
.mini-panel-border,.mini-toolbar{    
	border-color:#99bce8 !important;     
}

