*/
.operate_button:hover{
	color:#3DA2E1;
}
.prohibit_input .mini-textbox-border{
	background:#E6E6E6;
	border:0;
}

.fieldset{
	border:0;
	border-top:solid 1px #aaa;
	padding:5px 0 0 20px;
	background: #F4F4F4;
}

/*enquifyTrader*/
/*发行人行*/
.form_list_name{
	padding:10px 0 15px 20px;
	color:#777777;
	
}
.form_list_name>li{
	display:inline-block;
	margin-right:30px;
}
.input_width{
	width:220px !important;
}

.mini-buttonedit-border{
	height:19px;
}
/*浏览*/
.mini-buttonedit-button{
	height:15px;
}
.operate_button{
	outline:none;
	width:55px;
	height:22px;
	border:1px solid #AAAAAA;
	border-radius:5px;
	cursor:pointer;
	font-weight:bold;
	color:#434040;
	margin-left:10px;
}
.operate_button:hover{
	color:#3DA2E1;
	background:#E3E3E3;
}
.cstyle{
	color:#000;
}

/*参与，不参与，剔除*/
.strong{
  	display:inline-block;
  	border:1px solid #99BCE8;
  	width:15px;
  	height:15px;
  	margin:0 10px 0 5px;
  	vertical-align:middle; 
}

