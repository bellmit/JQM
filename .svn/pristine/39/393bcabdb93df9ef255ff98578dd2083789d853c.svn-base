	<select id="queryChargeCount" parameterClass="commonj.sdo.DataObject" resultMap="interestProcessInstructInfo">
	select t.* 
		   from T_ATS_INTERESTSWAP_INSTRUCT t
		  where 1=1
		  and t.vc_process_state = '5'
		  <isNotNull property="lTradeDateMin">
		  	  and t.l_trade_date &gt;= #lTradeDateMin#
          </isNotNull>
		  <isNotNull property="lTradeDateMax">
	          and t.l_trade_date &lt;= #lTradeDateMax#
          </isNotNull>
          <isNotNull property="vcProductCode">
	          and t.vc_Product_Code in ($vcProductCode$)
          </isNotNull>
          <isNotNull property="vcBusinType">
	          and t.vc_busin_type in ($vcBusinType$)
          </isNotNull>
          <isNotNull property="noCharge">
	          and l_instruct_id not in (select l_instruct_id from T_ATS_INTERESTSWAP_CHARGE)
          </isNotNull>
          <isNotNull property="charged">
	          and l_instruct_id in (select l_instruct_id from T_ATS_INTERESTSWAP_CHARGE)
          </isNotNull>  
	</select>
	
	<select id="queryChargeCountInfo" parameterClass="java.util.HashMap" resultMap="chargeCountInfo">
	select t.*              
		  from T_ATS_CHARGE_COUNT t
		  where 1=1
		  <isNotNull property="lInstructId">
	          and t.l_instruct_id = $lInstructId$
	      </isNotNull>
	      <isNotNull property="lChargeId">
	          and t.l_charge_id = $lChargeId$
	      </isNotNull>
