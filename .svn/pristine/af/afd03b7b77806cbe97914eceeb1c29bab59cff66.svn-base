----备付金保证金预估查询SQL-保证金-明细
select a.l_date, --日期
       a.l_fund_id, --基金序号
       a.vc_fund_code, --基金代码
       a.vc_fund_name, --基金名称
       a.VC_BUSIN_CAPTION, --业务类型
       a.VC_REPORT_CODE, --证券代码
       a.VC_STOCK_NAME, --证券名称
       a.EN_DEAL_BALANCE, --成交金额
       a.L_CAL_RATIO --保证金比例
  from O32_CJ.to32_deal_details a
 where 1 = 1
   --and a.l_date = '20170210'
   --and a.l_fund_id = '1001'
   --and a.vc_fund_code = '001199'
 --order by a.vc_report_code asc
