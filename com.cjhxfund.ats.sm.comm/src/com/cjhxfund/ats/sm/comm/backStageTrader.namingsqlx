    <!--交易指令管理-->
    <select id="queryTradeOrderData" resultMap="tradeOrderMap">
    	select c.b_check_flag,
		       c.l_date,
		       c.l_initdate,
		       c.l_tradeorderno,
		       c.vc_srctradeid,
		       c.c_sourcefrom,
		       c.vc_tablesource,
		       c.c_auditstatus,
		       c.c_orderstatus,
		       c.vc_tradeid,                        
		       c.l_opordertype,
		       c.c_tradetype,
		       c.c_cleartype,
		       c.vc_productcode,
		       c.vc_productname,
		       c.en_facevalue,
		       c.vc_buyerholderaccount,
		       c.vc_bholdershortname,
		       c.vc_boperholderaccount,
		       c.vc_sellerholderaccount,
		       c.vc_sholdershortname,
		       c.vc_soperholderaccount,
		       c.en_settlementamount,
		       c.l_tradedate,
		       c.l_settlementdate,
		       c.c_buyerstatus,
		       c.c_sellerstatus,
		       c.vc_boperorderstatus,
		       c.vc_soperorderstatus,
		       c.l_operid,
		       c.vc_opername,
		       c.l_auditoperid,
		       c.vc_auditopername,
		       c.l_settlementtype,
		       c.l_duesettledate,
		       c.en_duesettleamount,
		       c.l_duesettlemethod,
		       c.c_send_status,
		       c.c_match_flag,
		       c.vc_match_errmsg,
		       c.l_conf_operator,
		       c.vc_conf_operator_name,
		       c.en_cleanprice,
		       c.en_fullprice,
		       c.en_accruedinterest,
		       c.en_repodays,
		       c.c_distributionflag,
		       c.vc_reason,
		       c.c_paystock_dir,
		       c.c_from,
		       c.vc_selfconf_status,
		       c.vc_ravilconf_status
		  from (select b.*,
		               case
		                 when b.c_paystock_dir = '1' then
		                  c_buyerstatus
		                 else
		                  c_sellerstatus
		               end vc_selfconf_status,
		               case
		                 when b.c_paystock_dir = '2' then
		                  c_buyerstatus
		                 else
		                  c_sellerstatus
		               end vc_ravilconf_status
		          from (select 0 b_check_flag,
		                       l_date,
		                       l_initdate,
		                       l_tradeorderno,
		                       vc_srctradeid,
		                       c_sourcefrom,
		                       vc_tablesource,
		                       c_auditstatus,
		                       c_orderstatus,
		                       vc_tradeid,
		                       l_opordertype,
		                       c_tradetype,
		                       c_cleartype,
		                       vc_productcode,
		                       vc_productname,
		                       en_facevalue,
		                       vc_buyerholderaccount,
		                       vc_bholdershortname,
		                       vc_boperholderaccount,
		                       vc_sellerholderaccount,
		                       vc_sholdershortname,
		                       vc_soperholderaccount,
		                       en_settlementamount,
		                       l_tradedate,
		                       l_settlementdate,
		                       c_buyerstatus,
		                       c_sellerstatus,
		                       vc_boperorderstatus,
		                       vc_soperorderstatus,
		                       l_operid,
		                       vc_opername,
		                       l_auditoperid,
		                       vc_auditopername,
		                       l_settlementtype,
		                       l_duesettledate,
		                       en_duesettleamount,
		                       l_duesettlemethod,
		                       c_send_status,
		                       l_serial_no,
		                       c_match_flag,
		                       vc_match_errmsg,
		                       l_conf_operator,
		                       vc_conf_operator_name,
		                       en_cleanprice,
		                       en_fullprice,
		                       en_accruedinterest,
		                       en_repodays,
		                       c_distributionflag,
		                       vc_reason,
		                       nvl((select case
		                                    when instr(f.vc_shclearing_account,
		                                               a.vc_buyerholderaccount) > 0 then
		                                     '1'
		                                    else
		                                     '2'
		                                  end
		                             from trade.tfundinfo f
		                            where (instr(f.vc_shclearing_account,
		                                         a.vc_buyerholderaccount) > 0 or
		                                  instr(f.vc_shclearing_account,
		                                         a.vc_sellerholderaccount) > 0)
		                              and f.l_org_id = 0
		                              and f.l_dept_id = 0
		                              and rownum = 1),
		                           '1') c_paystock_dir,
		                       1 c_from
		                  from trade.tsqsjyinst a
		                  where trim(a.vc_srctradeid) is not null and a.l_date &gt;=(with calendarcalculate as
						 (select a.*, rank() over(partition by vc_tradeday_type order by l_date) rn
						          from trade.tmarkettradeday a
						         where a.vc_tradeday_type = '00'
						           and a.c_trade_flag = '1')
						  select l_date
						          from calendarcalculate b
						         where b.rn =
						               nvl((select c.rn + (-1)
						                     from calendarcalculate c
						                    where c.l_date =
						                          to_number(to_char(sysdate, 'yyyyMMdd'))),
						                   (select min(c.rn + (-1))
						                      from calendarcalculate c
						                     where c.l_date = to_number(to_char(sysdate, 'yyyyMMdd')))))) b) c
	</select>
